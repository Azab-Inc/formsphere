            steps{
                echo "Testing backend"
            }
        }
        stage("Test frontend") {
            steps {
                echo "Testing frontend"
            }
        }
        stage("Build backend") {
            steps {
                echo "Building backend"
            }
        }
        stage("Build frontend") {
            steps {
                echo "Building frontend"
            }
        }
    }

}